<h1>Authors</h1>

<% authors.forEach(function(author) { %>
  <div class="well">
    <h2>
      <a href="/authors/<%= author.id %>">
        <%= author.getFullName() %>
      </a>
    </h2>
  </div>
<% }) %>
<p>Sign up <a href="/authors/new">here</a> to be an author.</p>
